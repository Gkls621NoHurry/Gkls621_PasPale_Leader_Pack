		<Row Tag="LOC_ABILITY_EVE_COMBAT_AGAINST_STRONG_UNITS_gkls621_NAME" Language="zh_Hant_HK">
			<Text>拉普蘭武士道</Text>
		</Row>
		<Row Tag="LOC_ABILITY_EVE_COMBAT_AGAINST_STRONG_UNITS_gkls621_DESCRIPTION" Language="zh_Hant_HK">
